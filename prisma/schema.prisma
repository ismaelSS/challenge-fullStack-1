generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String    @id
  name         String
  email        String    @unique
  phone_number String
  password     String
  contact      Contact[]

  @@map("users")
}

model Contact {
  id           String @id
  name         String
  email        String
  phone_number String
  user         User   @relation(fields: [userId], references: [id])
  userId       String

  @@map("contact")
}
